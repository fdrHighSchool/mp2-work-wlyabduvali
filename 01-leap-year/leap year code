class Main {
  public static void main(String[] args) {

    if (isLeapYear(2020)) {
      System.out.println(" is a leap year.");
    } else {
      System.out.println(" is not a leap year.");
    }
}
  
  public static boolean isLeapYear(int year) {
    if ((year % 4 == 0) && (year % 100 != 0) || (year % 400 == 0)) {
      return true;
    } else {
      return false;
    }

  }  
}
